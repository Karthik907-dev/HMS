panel.add(label5);        JLabel label6 = new JLabel("Aadhar Number");        label6.setBounds(830,9,150,20);        label6.setFont(new Font("Tahoma",Font.BOLD,14));        panel.add(label6);        JButton button = new JButton("BACK");        button.setBounds(350,500,120,30);        button.setBackground(Color.BLACK);        button.setForeground(Color.white);        panel.add(button);        button.addActionListener(new ActionListener() {            @Override            public void actionPerformed(ActionEvent e) {                setVisible(false);            }        });        setUndecorated(true);        setSize(1000,600);        setLocation(350,230);        setLayout(null);        setVisible(true);    }    public static void main(String[] args) {        new Employee_info();    }}
